                    <div class="form-group">
                        <label for="author">위치정보</label>
                        <input type="text" class="form-control" id="locID" placeholder="위치정보를 선택하세요">
                    </div>
                    <label for="content">본문</label>
                    <div class="form-group" id="editor">

                    </div>
                </form>
                <a href="/boardV3" role="button" class="btn btn-secondary">취소</a>
                <button type="button" class="btn btn-primary" id="btn-save">등록</button>
            </div>
        </div>
    </div>
